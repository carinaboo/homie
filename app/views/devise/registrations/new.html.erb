<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :multipart => true }) do |f| %>
  <%= devise_error_messages! %>

  <div><br /><%= f.label :photo, "Upload picture" %>
   <%= f.file_field :photo %><br /><br /></div>

  <div><%= f.label :Name %>
  <%= f.text_field :username, :autofocus => true %><br /><br /></div>

  <div><%= f.label :email %>
  <%= f.email_field :email %><br /><br /></div>

  <div><%= f.label :password %>
  <%= f.password_field :password %><br /><br /></div>

  <div><%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %><br /><br /></div>

  <div><%= f.submit "Sign up" %></div>
<% end %>


