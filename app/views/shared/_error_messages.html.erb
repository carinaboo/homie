<% if @apartment.errors.any? %>
	<div id = "error_explanation">
		<div class="alert alert-error">
			The form contains <%= pluralize(@apartment.errors.count, "error") %>.
		</div>
		<ul>
		<% @apartment.errors.full_messages.each do |msg| %>
			<li> <%= msg %> </li>
		<% end %>
		</ul>
	</div>
<% end %>
