<% if current_user %>
  <h1>Welcome <%= current_user.username %></h1>
  <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
  <br>
  <%= link_to "Edit profile", edit_user_registration_path %>
  </div>
<% else %>
  <div class="home">
    <h1>Homie</h1>
    <h2 class="inline"><%= link_to "Log In", new_user_session_url %></h2>
    <h2 class="inline"><%= link_to "Sign Up", new_user_registration_url %></h2>
  </div>
<% end %>
